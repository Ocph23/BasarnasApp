
<MudDialog>
    <DialogActions>
        <MudButton Color="Color.Error" OnClick="@(()=> MudDialog.Close())">Close</MudButton>
    </DialogActions>
    <DialogContent>
        <MudCard>
            <div style="height:100%; width:100%; ">
                <div id="map1" style="width:100%; height:70vh"></div>
            </div>
        </MudCard>
    </DialogContent>
  
</MudDialog>
@inject IJSRuntime js
@code {


    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

	[Parameter]

	public string model { get; set; }


	protected override async Task OnInitializedAsync()
	{
        await js.InvokeVoidAsync("mapKejadian", model);
	}
}
